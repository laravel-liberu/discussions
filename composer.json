{
    "name": "laravel-liberu/discussions",
    "description": "Discussion board for Laravel Liberu",
    "keywords": [
        "laravel-liberu",
        "discussions"
    ],
    "homepage": "https://github.com/laravel-liberu/discussions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/enums": "^2.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/migrator": "^2.0",
        "laravel-liberu/track-who": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\Discussions\\": "src/",
            "LaravelLiberu\\Discussions\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLiberu\\Discussions\\AppServiceProvider",
                "LaravelLiberu\\Discussions\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
